:- #count {X : vrs(X), in(c1895_QingDynasty,X), in(c1895_Asia,X)} <= 0, pw.
pie(r0, A, 1) :- ir(X, A), in(c1895_QingDynasty, X), in(c1895_Asia, X), ix.
c(r0, A, 1) :- vr(X, A), in(c1895_QingDynasty, X), in(c1895_Asia, X), ix.

